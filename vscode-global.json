{
  "editor.fontSize": 15,
  "editor.wordWrap": "on",
  "terminal.integrated.fontSize": 14,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/_site": true,
    "**/.sass-cache": true,
    "**/.happypack": true,
    "**/node_modules": true,
    "**/.cache": true
  },
  "html.format.enable": false,
  "editor.renderWhitespace": "all",
  "window.zoomLevel": 0,
  "files.insertFinalNewline": true,
  "git.confirmSync": false,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "workbench.iconTheme": "vs-seti",
  "xmlTools.splitXmlnsOnFormat": false,
  "editor.tabSize": 2,
  "editor.fontFamily": " Fira Code",
  "editor.fontLigatures": true,
  "javascript.format.enable": false,
  "latex-workshop.chktex.enabled": true,
  "cSpell.userWords": [
    "\"babili\"",
    "ACLs",
    "Algolia",
    "Algolia's",
    "Bonomi",
    "Christoph",
    "Copyable",
    "Espen",
    "GPRS",
    "Henriksen",
    "Janton",
    "Kenzie",
    "Konstantin",
    "Laure",
    "Leaderboard",
    "Maervoet",
    "Magento",
    "Maxime",
    "Naudts",
    "Nigro",
    "Pojer",
    "Raev",
    "Repo",
    "Rutjes",
    "SDK's",
    "Saas",
    "Stanislawski",
    "Subheader",
    "Sylvain",
    "Unmount",
    "Utard",
    "Voyer",
    "algoliasearch",
    "backend",
    "changelog",
    "clearfix",
    "debounced",
    "execa",
    "heroku",
    "instantsearch",
    "lastmod",
    "lolex",
    "metalsmith",
    "postbuild",
    "prebuild",
    "preloaded",
    "referers",
    "retryable"
  ],
  "cSpell.ignorePaths": [
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/**",
    "**/.vscode/**",
    "settings.json",
    "typings"
  ],
  "cSpell.ignoreRegExpList": ["\\\\\\w*(\\[.*?\\])?(\\{.*?\\})?", "\\$.+?\\$"],
  "workbench.colorTheme": "Github",
  "emmet.excludeLanguages": [],
  "[cpp]": {
    "editor.quickSuggestions": false
  },
  "[c]": {
    "editor.quickSuggestions": false
  },
  "javascript.validate.enable": true,
  "git.enableSmartCommit": true,
  "files.associations": {
    ".babelrc": "json5",
    "*.js": "javascriptreact"
  },
  "editor.minimap.enabled": false,
  "workbench.colorCustomizations": {
    "activityBar.background": "#dcdcdc",
    "activityBar.foreground": "#646464",
    "activityBarBadge.background": "#424242",
    "activityBarBadge.foreground": "#dcdcdc",
    "statusBar.background": "#dcdcdc",
    "statusBar.foreground": "#6c6c6c",
    "statusBar.noFolderBackground": "#dcdcdc",
    "statusBar.noFolderForeground": "#6c6c6c",
    "button.background": "#646464",
    "button.hoverBackground": "#424242",
    "button.foreground": "#dcdcdc",
    "notification.background": "#dcdcdc",
    "notification.foreground": "#646464",
    "notification.buttonBackground": "#646464",
    "notification.buttonHoverBackground": "#424242",
    "notification.buttonForeground": "#dcdcdc",
    "notification.infoForeground": "#FFF",
    "notification.errorForeground": "#FFF",
    "notification.warningForeground": "#FFF"
  },
  "extensions.ignoreRecommendations": false,
  "gitlens.blame.line.enabled": false,
  "flow.useNPMPackagedFlow": true,
  "typescript.check.npmIsInstalled": false,
  "git.path": "/usr/local/bin/git",
  "workbench.startupEditor": "newUntitledFile",
  "prettier.eslintIntegration": true
}
